<script src='/scripts/artists.js' type='text/javascript'></script>
<script id="json-data"><%=@artists.to_json%></script>
<div class="container-fluid">
  <h1 class="text-center">
    artists
  </h1>
  <div class="text-center" id="artist-list">
    <div class="text-center" id="search">
      <i class="fa fa-search" aria-hidden="true"></i>
    </div>
    <div class="btn-group-vertical" role="group" aria-label="...">
      <% @artists.each do |artist| %>
        <button type="button" class="btn btn-default" id=<%= "#{artist.id}"%>>
          <%= artist.name %>
        </button>
      <% end %>
    </div>
  </div>
</div>
